<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8">
	<title>	Lumos &middot; EVS Project</title>
	<meta name="description" content="Kite Coming Soon HTML Template by Jewel Theme" >
	<meta name="author" content="Jewel Theme">
    <link rel="shortcut icon" href="http://www.google.com/s2/favicons?domain=www.iiitd.ac.in" type="image/x-icon">

	<meta name="viewport" content="width=device-width, initial-scale=1">
	<%= stylesheet_link_tag "bootstrap.min" %>
	<%= stylesheet_link_tag "font-awesome.min" %>
	<%= stylesheet_link_tag "style" %>
</head>
<body>


	<!-- Preloader -->
	<div id="preloader">
		<div id="loader">
			<div class="dot"></div>
			<div class="dot"></div>
			<div class="dot"></div>
			<div class="dot"></div>
			<div class="dot"></div>
			<div class="dot"></div>
			<div class="dot"></div>
			<div class="dot"></div>
			<div class="lading"></div>
		</div>
	</div><!-- /#preloader -->
	<!-- Preloader End-->
	

	<!-- Page Top Section -->
	<section id="page-top" class="section-style" data-background-image="/assets/background/page-top.jpg">
		<div class="pattern height-resize">
			<div class="container">
				<h1 class="site-title">
					Lumos
				</h1><!-- /.site-title -->
				<h3 class="section-name">
					<span>
						EVS Project
					</span>
				</h3><!-- /.section-name -->
				<h2 class="section-title" style="font-size:38px;">
					Some Statistics
				</h2><!-- /.Section-title  -->
				<div id="time_countdown" class="time-count-container">

					<div class="col-sm-3">
						<div class="time-box">
							<div class="time-box-inner dash days_dash animated" data-animation="rollIn" data-animation-delay="300">
								<span class="time-number">
									<span class="digit">0</span>
									<span class="digit">0</span>
									<span class="digit">0</span>
								</span>
								<span class="time-name">Days</span>
							</div>
						</div>
					</div>

					<div class="col-sm-3">
						<div class="time-box">
							<div class="time-box-inner dash hours_dash animated" data-animation="rollIn" data-animation-delay="600">
								<span class="time-number">
									<span class="digit">0</span>
									<span class="digit">0</span>	
								</span>
								<span class="time-name">Hours</span>
							</div>
						</div>
					</div>

					<div class="col-sm-3">
						<div class="time-box">
							<div class="time-box-inner dash minutes_dash animated" data-animation="rollIn" data-animation-delay="900">
								<span class="time-number">
									<span class="digit">0</span>
									<span class="digit">0</span>
								</span>
								<span class="time-name">Minutes</span>
							</div>
						</div>
					</div>

					<div class="col-sm-3">
						<div class="time-box">
							<div class="time-box-inner dash seconds_dash animated" data-animation="rollIn" data-animation-delay="1200">
								<span class="time-number">
									<span class="digit">0</span>
									<span class="digit">0</span>
								</span>
								<span class="time-name">Seconds</span>
							</div>
						</div>
					</div>
					
				</div><!-- /.time-count-container -->

				<p class="time-until" style="padding-bottom:60px;">
					<span>Time Since Electricity On</span>
				</p><!-- /.time-until -->



				<div class="next-section">
					<!-- <a class="go-to-about"><span></span></a> -->
				</div>
				
			</div><!-- /.container -->
		</div><!-- /.pattern -->		
	</section><!-- /#page-top -->
	<!-- Page Top Section  End -->

<!-- Subscribe Section -->
	<section id="subscribe">
		<div class="height-resize" style="margin-top:70px;">
			<div class="container" style="color:black;">
				<h2 class="section-title" style="font-size:50px; text-transform: capitalize;">
					Hourly-Usage Analysis
				</h2><!-- /.Section-title  -->
				<p class="section-description">
					Your average hourly electricity consumption
				</p><!-- /.section-description -->
				<br/>
				<br/>
				<div class="chart-wrapper">
					<div class="chart-inner">
						<div id="hourly-container" style="width:100%; height: 100%;">
						</div>
					</div>
				</div>
				</div><!-- /.container -->
			</div><!-- /.pattern -->

		</section><!-- /#subscribe -->
		<!-- Subscribe Section End -->
	
	<hr style="margin-left:50px;margin-right:50px;">
	<br/>
<!-- Subscribe Section -->
	<section id="subscribe">
		<div class="height-resize" style="margin-top:70px;">
			<div class="container" style="color:black;">
				<h2 class="section-title" style="font-size:50px;text-transform: capitalize;">
					Hour of Day vs Day of Week Analysis
				</h2><!-- /.Section-title  -->
				<p class="section-description">
					Average electricity consumption across Hour of Day - Day of Week
				</p><!-- /.section-description -->
				<br/>
				<br/>
				<div class="chart-wrapper">
					<div class="chart-inner">
						<div id="dow-container" style="width:100%; height: 100%;">
						</div>
					</div>
				</div>
				</div><!-- /.container -->
			</div><!-- /.pattern -->

		</section><!-- /#subscribe -->
		<!-- Subscribe Section End -->
	<hr style="margin-left:50px;margin-right:50px;">
	<br/>


<!-- Subscribe Section -->
	<section id="subscribe">
		<div class="height-resize" style="margin-top:70px;">
			<div class="container" style="color:black;">
				<h2 class="section-title" style="font-size:50px;text-transform: capitalize;">
					Geographical Usage
				</h2><!-- /.Section-title  -->
				<p class="section-description">
					Consumption stats across regions
				</p><!-- /.section-description -->
				<br/>
				<br/>
				<div class="chart-wrapper">
					<div class="chart-inner">
						<div id="borderless-world" style="height: 500px; width: 100%; padding: 8px; border-radius:3px;"></div>
						</div>
					</div>
				</div>
				</div><!-- /.container -->
			</div><!-- /.pattern -->

		</section><!-- /#subscribe -->
		<!-- Subscribe Section End -->



	<!-- About Us Section -->
	<section id="about" class="section-style" data-background-image="assets/background/about-us.jpg">
		<div class="pattern height-resize"> 
			<div class="container">
				<h3 class="section-name">
					<span>
						About Us
					</span>
				</h3><!-- /.section-name -->
				<h2 class="section-title">
					We Are dedicated
				</h2><!-- /.Section-title  -->
				<p class="section-description">
					Our Team: Ambar Pal, Rounaq Jhunjhunu Wala, Divam Gupta, Anshuman Suri, Ishita Verma, Nitika Saran, Palash Bansal, Aashay Mittal, Aishwarya Jaiswal, Harshvardhan Kalra, Mansi Goel, Satyam Kumar, Aishwarya Raj, Tanya Choudhary, Shrey Bagroy
				</p><!-- /.section-description -->
				</div><!-- /.team-container -->

				<div class="next-section" style="height:100px;">
				</div><!-- /.next-section -->

			</div><!-- /.container -->
		</div><!-- /.pattern -->		
	</section><!-- /#about -->
	<!-- About Us Section End -->

		<!-- Footer Section -->
		<footer id="footer-section">
			<p class="copyright">
				&copy; <a>Lumos</a> 2016, All Rights Reserved.
			</p>
		</footer>
		<!-- Footer Section End -->



		<!-- jQuery Library -->
		<%= javascript_include_tag "jquery-2.1.0.min", "data-turbolinks-track" => true  %>		
		<%= javascript_include_tag "modernizr-2.8.0.min", "data-turbolinks-track" => true  %>		
		<%= javascript_include_tag "plugins", "data-turbolinks-track" => true  %>		
		<%= javascript_include_tag "functions", "data-turbolinks-track" => true  %>		
		<%= javascript_include_tag "jquery.ajaxchimp.min", "data-turbolinks-track" => true  %>		


		<script src="http://maps.google.com/maps/api/js?sensor=false&libraries=places"></script>
		<%= javascript_include_tag "hourly-usage", "data-turbolinks-track" => true  %>		
		<%= javascript_include_tag "dow-hod", "data-turbolinks-track" => true  %>		
		<%= javascript_include_tag "geo-map", "data-turbolinks-track" => true  %>		
		<%= javascript_include_tag "misc", "data-turbolinks-track" => true  %>		
		<script src="https://code.highcharts.com/highcharts.js"></script>
		<script src="https://code.highcharts.com/highcharts-more.js"></script>
		<script src="https://code.highcharts.com/modules/exporting.js"></script>
	</body>
	</html>
